{{#each peliculas}}
<div class="col-xs-6 col-sm-4 col-md-3 text-center">
        <a href="peliculas/{{this.id}}">
            <img src="{{this.poster}}" style="height:200px"/>
            <h4 style="min-height:45px;margin:5px 0 10px 0">
            {{this.title}}
            </h4>
        </a>
</div>
{{/each}}
